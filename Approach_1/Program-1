package approach1_programming;

public class Ballon 
{
	public String color;
}

package approach1_mainclass;

import approach1_programming.Ballon;
public class MainClass5 
{
	public static void main(String[] args)
	{
		Ballon b1=new Ballon();
		Ballon b2=new Ballon();
		b1.color="Gold";
		b2.color="Silver";
		System.out.println("Ballon 1 Color is :"+b1.color);
		System.out.println("Ballon 2 color is :"+b2.color);
		
	}
}

//Output :
Ballon 1 Color is :Gold
Ballon 2 color is :Silver
