

package approach2_programming;
public class Chair 
{
	public String color;
	public int height;
	
	public void validColour(String s)
	{
		if(s.equalsIgnoreCase("Blue")|| s.equalsIgnoreCase("White")|| s.equalsIgnoreCase("Orange")|| s.equalsIgnoreCase("Red"))
		{
			System.out.println("Valid Chair Color ");
		}
		else
		{
			System.out.println("Invalid Chair color");
		}
	}
	public void validHeight(int h)
	{
		if(h>=50 && h<=100)
		{
			System.out.println("valid Chair Height data");
		}
		else
		{
			System.out.println("Invalid Chair Height data");
		}
	}
}

package approach2_mainclass;

import approach3_programming.Chair;
public class MainClass4 
{
	public static void main()
	{
		Chair c1=new Chair("Red", 5 );
		
		
		Chair c2=new Chair("White", 10);
		
	}
}

